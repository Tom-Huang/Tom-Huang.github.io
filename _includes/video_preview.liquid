{% if url contains '.mp4' or url contains '.webm' %}
  <video class="preview z-depth-1 rounded" width="100%" autoplay muted loop playsinline>
    <source src="{{ url }}" type="video/mp4">
    Your browser does not support the video tag.
  </video>
{% else %}
  <img class="preview z-depth-1 rounded" src="{{ url }}" alt="{{ alt }}">
{% endif %} 